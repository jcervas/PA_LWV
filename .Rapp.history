`CI` <- #
    function(x, se, level=0.95) {#
        if (level==0.95) z <- 1.96#
        if (level==0.90) z <- 1.645#
        ci <- rbind(x, x - (z * se), x + (z * se))#
        rownames(ci) <- c("beta", "lower", "upper")#
        return(ci)#
    }#
